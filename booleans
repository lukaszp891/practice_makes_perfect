# boolean_1 = (3 < 4 || false) && (false || true)
boolean_1 = true

# boolean_2 = !true && (!true || 100 != 5**2)
boolean_2 = false

# boolean_3 = true || !(true || false)
boolean_3 = true

# test_1 should be true
test_1 = (1 < 2) && (2 < 3)

# test_2 = should be true
test_2 = (1 == 1) || (2 == 2)

# test_3 = should be false
test_3 = !(2 == 2)
